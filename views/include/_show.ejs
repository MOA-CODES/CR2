<%for (var i = 0; i<users.length; i++){%>

<!-- INCASE i come back and i am confused of where "users" originated from 
    its from config.env , 
    where i specified that "users" is the name of the database
    (speculation) it also knows users because i specified the connection
-->

<tr>
    <td><%= i+1 %></td>
    <td><%= users[i].name %></td>                            
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update" title="1">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id="<%= users[i]._id%>">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<%}%>
